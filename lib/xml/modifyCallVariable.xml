    xmlns:ser="http://service.admin.ws.five9.com/">
    <soapenv:Header/>
    <soapenv:Body>
        <ser:modifyCallVariable>
            <!--Optional:-->
            <variable>
                <!--Optional:-->
                <applyToAllDispositions>?</applyToAllDispositions>
                <!--Optional:-->
                <defaultValue>?</defaultValue>
                <!--Optional:-->
                <description>?</description>
                <!--Zero or more repetitions:-->

                <!--Optional:-->
                <group>?</group>
                <!--Optional:-->
                <name>?</name>
                <!--Optional:-->
                <reporting>?</reporting>
                <!--Zero or more repetitions:-->
                <restrictions>
                    <!--Optional:-->
                    <type>?</type>
                    <!--Optional:-->
                    <value>?</value>
                </restrictions>
                <!--Optional:-->
                <sensitiveData>?</sensitiveData>
                <!--Optional:-->
                <type>?</type>
            </variable>
        </ser:modifyCallVariable>
    </soapenv:Body>
</soapenv:Envelope>